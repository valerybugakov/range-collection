{
  extends: [
    'airbnb'
  ],
  parser: 'babel-eslint',
  plugins: [
    'babel',
  ],
  env: {
    node: true,
    es6: true,
    jest: true,
  },
  rules: {
    'max-len': [2, 100],
    'semi': [2, 'never'],
    'no-console': 0,
    'no-plusplus': 0,
    'import/extensions': 0,

    'import/named': 2,
    'import/no-unresolved': 2,
  },
}
